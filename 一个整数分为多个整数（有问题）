#include<stdio.h>
int main()
{
	int left = 1, right = 2;
	int sum = 3;
	int n;
	scanf("%d", &n);
	while (left < right)
	{
		if (sum < n)
		{
			right++;
			sum += right;
		}
		else if (sum > n)
		{
			sum -= left;
			left++;
		}
		else
		{
			printf("[%d,%d]\n", left, right);
			right++;
			sum += right;
		}
	}
