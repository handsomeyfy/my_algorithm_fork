#include<stdio.h>
int main()
{
	int i, n, t,m,j;
	int a[1000] = { 0 };
	scanf_s("%d", &n)ï¼›
	
	for (i = 1; i <= n; i++)
		scanf_s("%d", &a[i]);

	for (i = 1; i <= n - 1; i++)
	 {
			for (j = 1; j <= n - i; j++)
			{
				if (a[i] >= a[i + j])
				{
					m = a[i];
					a[i] = a[i + j];
					a[i + j] = m;
        }

				else
					continue;
			}
		}
		for (i = 1; i <= n; i++)
		{
			printf("%d ", a[i]);
		}

		printf("\n");
		
	

	
    return 0;
}



